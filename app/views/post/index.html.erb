<h1>Post#index</h1>
<p>Find me in app/views/post/index.html.erb</p>
<a href="/post/new">게시물 작성하기</a>

<% @posts.each do |p| %>
	<p> 제목 : <%= p.title %> </p>
	<p> 내용 : <%= p.content %> </p>
	<a href="/post/show/<%= p.id %>">보기</a>
	<a href="/post/edit/<%= p.id %>">수정</a>
	<a href="/post/destroy/<%= p.id %>">삭제</a>
	<hr>
<% end %>